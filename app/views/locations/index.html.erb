<h1>Locations#index</h1>
<p>Find me in app/views/locations/index.html.erb</p>
<table>
  <thead>
    <th>Name</th>
    <th>Address</th>
    <th>Capacity</th>
  </thead>
  <tbody>
    <% @locations.each do |f|%>
      <tr>
        <td><%= f.name%></td>
        <td><%= f.address%></td>
        <td><%= f.capacity%></td>
        <td><%= link_to "Display", show_locations_path(f) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to "New Location", new_location_path %>